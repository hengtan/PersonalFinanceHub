{
  "name": "personal-finance-hub-backend",
  "version": "1.0.0",
  "description": "Backend for Personal Finance Hub - A comprehensive financial management platform",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "start": "node dist/server.js",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "migration:create": "typeorm migration:create",
    "migration:run": "typeorm migration:run",
    "migration:revert": "typeorm migration:revert",
    "seed": "ts-node src/database/seeds/index.ts",
    "docker:build": "docker build -t pfh-backend .",
    "docker:run": "docker run -p 3333:3333 pfh-backend",
    "metrics": "tsx src/scripts/metrics-test.ts",
    "health": "curl http://localhost:3333/health"
  },
  "keywords": [
    "personal-finance",
    "fintech",
    "nodejs",
    "typescript",
    "microservices",
    "event-driven",
    "cqrs"
  ],
  "author": "Personal Finance Hub Team",
  "license": "MIT",
  "dependencies": {
    "@fastify/cookie": "^9.4.0",
    "@fastify/cors": "^8.5.0",
    "@fastify/helmet": "^10.1.1",
    "@fastify/rate-limit": "^8.1.1",
    "@fastify/swagger": "^8.15.0",
    "@fastify/swagger-ui": "^1.10.2",
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/auto-instrumentations-node": "^0.40.3",
    "@opentelemetry/exporter-jaeger": "^1.17.2",
    "@opentelemetry/instrumentation": "^0.46.0",
    "@opentelemetry/instrumentation-fastify": "^0.33.0",
    "@opentelemetry/instrumentation-http": "^0.46.0",
    "@opentelemetry/instrumentation-pg": "^0.37.0",
    "@opentelemetry/instrumentation-redis": "^0.37.0",
    "@opentelemetry/sdk-node": "^0.46.0",
    "@types/node-cron": "^3.0.11",
    "ajv": "^8.12.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "date-fns": "^3.0.6",
    "dotenv": "^16.3.1",
    "fastify": "^4.25.2",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "kafkajs": "^2.2.4",
    "lodash": "^4.17.21",
    "mongodb": "^6.3.0",
    "mongoose": "^8.0.3",
    "node-cron": "^4.2.1",
    "nodemailer": "^6.9.8",
    "pg": "^8.11.3",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "prom-client": "^15.1.0",
    "reflect-metadata": "^0.2.1",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.5",
    "@types/nodemailer": "^6.4.14",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tsconfig-paths": "^4.1.0",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/personal-finance-hub"
  },
  "bugs": {
    "url": "https://github.com/your-org/personal-finance-hub/issues"
  },
  "homepage": "https://github.com/your-org/personal-finance-hub#readme"
}
